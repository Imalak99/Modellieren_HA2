{
  "$id": "http://modeling/dach.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Dach",
  "type": "object",
  "patternProperties": {
    "^Dach-\\d+$": {
      "type": "object",
      "required": ["ID", "Neigung"],
      "properties": {
        "ID": {
          "type": "string"
        },
        "Neigung": {
          "type": "number"
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}