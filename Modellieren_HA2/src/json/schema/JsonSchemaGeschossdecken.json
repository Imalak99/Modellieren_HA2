{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^Geschossdecke\\d+$": {
      "type": "object",
      "properties": {
        "Ifc Material": {
          "type": "string"
        },
        "ID": {
          "type": "string"
        },
        "Dicke": {
          "type": "number"
        },
        "Volumen": {
          "type": "number"
        }
      },
      "required": ["Ifc Material", "ID", "Dicke", "Volumen"],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
