{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^Dach\\d+$": {
      "type": "object",
      "properties": {
        "ID": {
          "type": "string"
        },
        "Neigung": {
          "type": "number"
        }
      },
      "required": ["ID", "Neigung"],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
